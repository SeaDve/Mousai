<?xml version="1.0" encoding="utf-8"?>
<schemalist>
  <schema path="/io/github/seadve/Mousai/" id="@app-id@" gettext-domain="@gettext-package@">
    <key name="window-width" type="i">
      <default>600</default>
      <summary>Window width</summary>
      <description></description>
    </key>
    <key name="window-height" type="i">
      <default>400</default>
      <summary>Window height</summary>
      <description></description>
    </key>
    <key name="window-maximized" type="b">
      <default>false</default>
      <summary>Whether the window is maximized</summary>
      <description></description>
    </key>
    <key name="saved-recordings" type="s">
      <default>"[]"</default>
      <summary>Recordings to be recognized</summary>
      <description></description>
    </key>
    <key name="preferred-audio-source" type="s">
      <choices>
        <choice value="microphone"/>
        <choice value="desktop-audio"/>
      </choices>
      <default>"microphone"</default>
      <summary>Preferred audio source to use in recording audio</summary>
      <description></description>
    </key>
  </schema>
</schemalist>
